<?xml version="1.0" encoding="UTF-8" ?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {{ range .Site.AllPages }}
    <url>
      <loc>{{ .Permalink }}</loc>
      <lastmod>{{ safeHTML .Lastmod.Format "2006-01-02T15:04:05-07:00" }}</lastmod>
      <changefreq>weekly</changefreq>
      <priority>0.8</priority>
    </url>
  {{ end }}
</urlset>
